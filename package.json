{
  "name": "hos-find-be",
  "version": "1.0.0",
  "description": "Backend API for HosFind - Hostel Finding Application",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "yarn typeorm migration:generate -d src/config/database.ts",
    "migration:run": "yarn typeorm migration:run -d src/config/database.ts",
    "migration:revert": "yarn typeorm migration:revert -d src/config/database.ts",
    "db:seed": "ts-node src/scripts/seed.ts",
    "db:setup": "ts-node src/scripts/setup-db.ts",
    "admin:create": "ts-node src/scripts/create-admin.ts"
  },
  "keywords": [
    "hostel",
    "finding",
    "api",
    "express",
    "typescript",
    "postgresql",
    "typeorm"
  ],
  "author": "HosFind Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "typeorm": "^0.3.17",
    "reflect-metadata": "^0.1.13",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "@types/node": "^20.8.10",
    "@types/express": "^4.17.20",
    "@types/cors": "^2.8.15",
    "@types/morgan": "^1.9.9",
    "@types/bcryptjs": "^2.4.5",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/pg": "^8.10.7",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
