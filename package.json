{
  "name": "hos-find-be",
  "version": "1.0.0",
  "description": "Backend API for HosFind - Hostel Finding Application",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "start:prod": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "postinstall": "yarn build",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "yarn typeorm migration:generate -d src/config/data-source.ts",
    "migration:run": "yarn typeorm migration:run -d src/config/data-source.ts",
    "migration:revert": "yarn typeorm migration:revert -d src/config/data-source.ts",
    "db:seed": "ts-node src/scripts/seed.ts",
    "db:setup": "ts-node src/scripts/setup-db.ts",
    "admin:create": "ts-node src/scripts/create-admin.ts",
    "sample:create": "ts-node src/scripts/create-sample-properties.ts",
    "remove:gender": "ts-node src/scripts/remove-gender-column.ts",
    "db:check": "ts-node src/scripts/check-database.ts",
    "test:like": "ts-node src/scripts/test-like-entity.ts",
    "test:server-like": "ts-node src/scripts/test-server-like.ts",
    "seed:room-types": "ts-node src/scripts/seed-room-types-fixed.ts",
    "update:coordinates": "ts-node src/scripts/update-properties-coordinates.ts",
    "seed:notifications": "ts-node src/scripts/seed-notifications.ts"
  },
  "keywords": [
    "hostel",
    "finding",
    "api",
    "express",
    "typescript",
    "postgresql",
    "typeorm"
  ],
  "author": "HosFind Team",
  "license": "MIT",
  "dependencies": {
    "@types/multer": "^2.0.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "expo-notifications": "^0.31.4",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.5",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.7",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
